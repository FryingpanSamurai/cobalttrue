{
	"name": "CobaltTrue",
	"colors": {
		"editor.background": "#121418",
		"editor.foreground": "#626d7a",
		"activityBarBadge.background": "#007acc",
		"sideBarTitle.foreground": "#bbbbbb"
	},
	"tokenColors": [
		{
			"name": "Comment",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"fontStyle": "",
				"foreground": "#448568"
			}
		},
		{
			"name": "Variables",
			"scope": [
				"variable",
				"string constant.other.placeholder"
			],
			"settings": {
				"foreground": "#55dde0"
			}
		},
		{
			"name": "Colors",
			"scope": [
				"constant.other.color"
			],
			"settings": {
				"foreground": "#778DA9"
			}
		},
		{
			"name": "Invalid",
			"scope": [
				"invalid",
				"invalid.illegal"
			],
			"settings": {
				"foreground": "#FF5370"
			}
		},
		{
			"name": "Keyword, Storage",
			"scope": [
				"keyword",
				"storage.type",
				"storage.modifier"
			],
			"settings": {
				"foreground": "#3a8394"
			}
		},
		{
			"name": "Keyword Operator",
			"scope": [
				"keyword.operator"
			],
			"settings": {
				"foreground": "#2690a8",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Operator, Misc",
			"scope": [
				"constant.other.color",
				"punctuation",
				"meta.tag",
				"punctuation.definition.tag",
				"punctuation.separator.inheritance.php",
				"punctuation.definition.tag.html",
				"punctuation.definition.tag.begin.html",
				"punctuation.definition.tag.end.html",
				"punctuation.section.embedded"
			],
			"settings": {
				"foreground": "#778DA9"
			}
		},
		{
			"name": "Parenthesis",
			"scope": [
				"punctuation.definition.arguments.begin",
				"punctuation.definition.arguments.end"
			],
			"settings": {
				"foreground": "#65BA3D",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Tag",
