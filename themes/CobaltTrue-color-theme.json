			"settings": {
				"foreground": "#666",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Function Param",
			"scope": [
				"variable.parameter.function",
				"variable.parameter.function-call"
			],
			"settings": {
				"foreground": "#45b9bb",
				"fontStyle": ""
			}
		},
		{
			"name": "String, Symbols, Markup Heading",
			"scope": [
				"string",
				"constant.other.symbol",
				"constant.other.key",
